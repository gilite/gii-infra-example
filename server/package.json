    "nexus": "^1.3.0",
    "prisma": "^3.11.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@prisma/client": "^3.11.0",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "ts-node": "^10.7.0"
  }
}
